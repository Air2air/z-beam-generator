{
  "materialProfile": {
    "name": "material",
    "version": "1.6",
    "schemaType": "MaterialProfile",
    "generatorConfig": {
      "jsonld": {
        "schemaType": "Product"
      },
      "contentGeneration": {
        "dynamicSectionsFromFrontmatter": true,
        "fieldContentMapping": {
          "properties": "Detail the physical and chemical properties of {subject} relevant to laser cleaning",
          "laserParameters": "Explain the optimal laser parameters for cleaning {subject}, including wavelength, fluence, and pulse duration",
          "applications": "Describe the key applications where {subject} is processed using laser cleaning",
          "challenges": "Analyze the specific challenges in laser cleaning {subject} and their technical solutions",
          "safetyConsiderations": "Outline the safety considerations and protocols when laser cleaning {subject}"
        },
        "introductionPrompt": "Provide a comprehensive technical overview of {subject} as a material in laser cleaning applications"
      },
      "useResearchModule": true,
      "research": {
        "fields": [
          "properties",
          "composition",
          "compatibility",
          "regulatoryStandards",
          "laserParameters",
          "environmentalConsiderations",
          "chemicalProperties"
        ],
        "dataStructure": {
          "chemicalProperties": {
            "type": "object",
            "propertyFormat": "keyValue",
            "itemStructure": {
              "symbol": "string",
              "formula": "string",
              "materialType": "string"
            }
          },
          "composition": {
            "type": "array",
            "itemStructure": {
              "component": "string",
              "percentage": "string",
              "type": "string",
              "formula": "string"
            }
          },
          "properties": {
            "type": "object",
            "propertyFormat": "keyValue"
          },
          "laserParameters": {
            "type": "object",
            "propertyFormat": "keyValue"
          },
          "environmentalConsiderations": {
            "type": "object",
            "propertyFormat": "keyValue"
          }
        }
      }
    },
    "profile": {
      "category": {
        "type": "string",
        "required": true,
        "description": "Material category (metal, ceramic, semiconductor, plastic, composite, glass, masonry, stone, wood)",
        "enum": ["metal", "ceramic", "semiconductor", "plastic", "composite", "glass", "masonry", "stone", "wood"],
        "example": "semiconductor"
      },
      "chemicalProperties": {
        "type": "object",
        "required": true,
        "properties": {
          "symbol": {
            "type": "string",
            "description": "Symbol or abbreviation for the material (e.g., Ka for Kaolin, Al for Aluminum)",
            "example": "Ka"
          },
          "formula": {
            "type": "string",
            "description": "Chemical formula of the material",
            "example": "Al₂Si₂O₅(OH)₄"
          },
          "materialType": {
            "type": "string",
            "enum": ["element", "compound", "alloy", "composite", "polymer", "ceramic", "mineral"],
            "description": "Chemical classification of the material",
            "example": "compound"
          }
        },
        "description": "Chemical properties for badge symbol and identification"
      },
      "properties": {
        "type": "object",
        "required": true,
        "properties": {
          "density": {
            "type": "string",
            "description": "Material density",
            "example": "2.7 g/cm³"
          },
          "meltingPoint": {
            "type": "string",
            "description": "Melting point",
            "example": "660°C"
          },
          "thermalConductivity": {
            "type": "string",
            "description": "Thermal conductivity",
            "example": "237 W/m·K"
          },
          "laserType": {
            "type": "string",
            "description": "Laser type for cleaning",
            "example": "Hafnium-doped fiber"
          },
          "wavelength": {
            "type": "string",
            "description": "Laser wavelength",
            "example": "1064nm ±2nm"
          },
          "fluenceRange": {
            "type": "string",
            "description": "Fluence range",
            "example": "0.1–15 J/cm²"
          },
          "chemicalFormula": {
            "type": "string",
            "description": "Chemical formula or scientific symbol",
            "example": "Al₂O₃"
          }
        },
        "description": "Physical, chemical, and laser cleaning parameters"
      },
      "composition": {
        "type": "array",
        "description": "Chemical or structural composition of the material",
        "items": {
          "type": "object",
          "properties": {
            "component": {
              "type": "string",
              "description": "Component name (element, compound, or structural component)"
            },
            "percentage": {
              "type": "string",
              "description": "Percentage or proportion as a string with unit"
            },
            "type": {
              "type": "string",
              "enum": ["element", "compound", "structural"],
              "description": "Type of component (element for metals, structural for woods, etc.)"
            },
            "formula": {
              "type": "string",
              "description": "Chemical formula or scientific symbol (e.g., Al₂O₃, SiO₂, Fe)"
            }
          }
        }
      },
      "compatibility": {
        "type": "array",
        "required": true,
        "items": {
          "type": "object",
          "properties": {
            "material": {
              "type": "string",
              "required": true,
              "description": "Compatible material",
              "example": "Stainless Steel"
            },
            "application": {
              "type": "string",
              "required": true,
              "description": "Application or process",
              "example": "Welding"
            }
          }
        },
        "description": "Compatible materials and applications"
      },
      "images": {
        "type": "object",
        "required": true,
        "properties": {
          "hero": {
            "type": "object",
            "properties": {
              "alt": {
                "type": "string",
                "required": true,
                "description": "Alt text for hero image",
                "example": "Close-up of Alumina surface being laser cleaned showing material texture and reflectivity"
              },
              "url": {
                "type": "string",
                "required": true,
                "description": "URL for hero image",
                "example": "https://www.z-beam.com/images/alumina-laser-cleaning-hero.jpg"
              }
            }
          },
          "closeup": {
            "type": "object",
            "properties": {
              "alt": {
                "type": "string",
                "required": true,
                "description": "Alt text for closeup image",
                "example": "Detailed view of Alumina microstructure after laser cleaning process"
              },
              "url": {
                "type": "string",
                "required": true,
                "description": "URL for closeup image",
                "example": "https://www.z-beam.com/images/alumina-laser-cleaning-closeup.jpg"
              }
            }
          }
        },
        "description": "Image descriptions for article visuals"
      }
    },
    "technicalSpecifications": {
      "type": "object",
      "required": false,
      "properties": {
        "laserType": {
          "type": "string",
          "description": "Type of laser used",
          "example": "Hafnium-doped fiber"
        },
        "wavelength": {
          "type": "string",
          "description": "Laser wavelength",
          "example": "1064nm ±2nm"
        },
        "powerRange": {
          "type": "string",
          "description": "Power range",
          "example": "50–1000W pulsed"
        },
        "fluenceRange": {
          "type": "string",
          "description": "Fluence range",
          "example": "0.1–15 J/cm²"
        },
        "pulseDuration": {
          "type": "string",
          "description": "Pulse duration",
          "example": "10–200ns"
        }
      },
      "description": "Technical parameters for material/thesaurus articles"
    },
    "validation": {
      "frontmatter": {
        "requiredFields": [
          "name",
          "description",
          "website",
          "author",
          "keywords",
          "category",
          "chemicalProperties",
          "properties",
          "composition",
          "compatibility",
          "regulatoryStandards",
          "images"
        ]
      }
    }
  }
}
